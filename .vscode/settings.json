{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "files.associations": {
    "*.snapshots": "json"
  },
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    ".tmp": true
  },
  "files.exclude": {
    ".tmp": true
  },
  "search.exclude": {
    ".tmp": true,
    "typings": true
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/pbiviz.json"
      ],
      "url": "./packages/powerbi/.api/v1.10.0/schema.pbiviz.json"
    },
    {
      "fileMatch": [
        "/capabilities.json"
      ],
      "url": "./packages/powerbi/.api/v1.10.0/schema.capabilities.json"
    },
    {
      "fileMatch": [
        "/dependencies.json"
      ],
      "url": "./packages/powerbi/.api/v1.10.0/schema.dependencies.json"
    }
  ],
  "typescript.extension.sortImports.sortMethod": "path",
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
