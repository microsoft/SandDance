{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "../../../sample-data/titanicmaster.tsv",
        "format": {
            "type": "tsv"
        }
    },
    "height": 300,
    "width": 300,
    "mark": "bar",
    "encoding": {
        "facet": {
            "field": "Age",
            "type": "quantitative",
            "bin": {
                "maxbins": 30
            },
            "columns": 10
        },
        "x": {
            "field": "Class",
            "type": "nominal"
        },
        "y": {
            "field": "TicketCost",
            "type": "quantitative",
            "aggregate": "sum"
        }
    }
}