{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "data": [
    {
      "name": "source_00",
      "url": "https://sanddance.js.org/sample-data/titanicmaster.tsv",
      "format": {
        "type": "tsv"
      },
      "transform": [
        {
          "type": "extent",
          "field": "Age",
          "signal": "child_bin_maxbins_20_Age_extent"
        },
        {
          "type": "bin",
          "field": "Age",
          "as": [
            "bin_maxbins_20_Age",
            "bin_maxbins_20_Age_end"
          ],
          "signal": "child_bin_maxbins_20_Age_bins",
          "extent": {
            "signal": "child_bin_maxbins_20_Age_extent"
          },
          "maxbins": 20
        }
      ]
    },
    {
      "name": "source_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "bin_maxbins_20_Age",
            "bin_maxbins_20_Age_end",
            "Joined"
          ],
          "ops": [
            "sum"
          ],
          "fields": [
            "TicketCost"
          ],
          "as": [
            "sum_TicketCost"
          ]
        },
        {
          "type": "filter",
          "expr": "isValid(datum[\"bin_maxbins_20_Age\"]) && isFinite(+datum[\"bin_maxbins_20_Age\"])"
        },
        {
          "type": "extent",
          "field": "__count",
          "signal": "maxcount"
        }
      ],
      "source": "source_00"
    },
    {
      "name": "facet_domain",
      "source": "source_0",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "Joined"
          ]
        }
      ]
    },
    {
      "name": "facet_domain_row",
      "transform": [
        {
          "type": "sequence",
          "start": 0,
          "stop": 1
        }
      ]
    },
    {
      "name": "facet_domain_column",
      "transform": [
        {
          "type": "sequence",
          "start": 0,
          "stop": {
            "signal": "length(data(\"facet_domain\"))"
          }
        }
      ]
    },
    {
      "name": "seq",
      "transform": [
        {
          "type": "sequence",
          "start": {
            "signal": "child_bin_maxbins_20_Age_bins.start"
          },
          "stop": {
            "signal": "child_bin_maxbins_20_Age_bins.stop"
          },
          "step": {
            "signal": "child_bin_maxbins_20_Age_bins.step"
          }
        }
      ]
    }
  ],
  "signals": [
    {
      "name": "child_width",
      "value": 300
    },
    {
      "name": "child_height",
      "value": 300
    },
    {
      "name": "bandWidth",
      "update": "bandwidth('quantBand')"
    },
    {
      "name": "bandPadding",
      "value": 0.1
    }
  ],
  "layout": {
    "padding": 20,
    "bounds": "full",
    "align": "all"
  },
  "marks": [
    {
      "name": "facet-title",
      "type": "group",
      "role": "column-title",
      "title": {
        "text": "Joined",
        "style": "guide-title",
        "offset": 10
      }
    },
    {
      "name": "row_header",
      "type": "group",
      "role": "row-header",
      "from": {
        "data": "facet_domain_row"
      },
      "encode": {
        "update": {
          "height": {
            "signal": "child_height"
          }
        }
      },
      "axes": [
        {
          "scale": "y",
          "orient": "left",
          "grid": false,
          "title": "Age (binned)",
          "labelOverlap": true,
          "tickCount": {
            "signal": "ceil(child_height/10)"
          },
          "zindex": 0
        }
      ]
    },
    {
      "name": "column_footer",
      "type": "group",
      "role": "column-footer",
      "from": {
        "data": "facet_domain_column"
      },
      "encode": {
        "update": {
          "width": {
            "signal": "child_width"
          }
        }
      },
      "axes": [
        {
          "scale": "x",
          "orient": "bottom",
          "grid": false,
          "title": "Sum of TicketCost",
          "labelFlush": true,
          "labelOverlap": true,
          "tickCount": {
            "signal": "ceil(child_width/40)"
          },
          "zindex": 0
        }
      ]
    },
    {
      "name": "cell",
      "type": "group",
      "title": {
        "text": {
          "signal": "''+parent[\"Joined\"]"
        },
        "style": "guide-label",
        "frame": "group",
        "offset": 10
      },
      "style": "cell",
      "from": {
        "facet": {
          "name": "facet",
          "data": "source_00",
          "groupby": [
            "Joined"
          ]
        }
      },
      "sort": {
        "field": [
          "datum[\"Joined\"]"
        ],
        "order": [
          "ascending"
        ]
      },
      "encode": {
        "update": {
          "width": {
            "signal": "child_width"
          },
          "height": {
            "signal": "child_height"
          }
        }
      },
      "marks": [
        {
          "name": "bandfacet",
          "type": "group",
          "from": {
            "facet": {
              "name": "bandfacet_0",
              "data": "facet",
              "groupby": [
                "Joined",
                "bin_maxbins_20_Age"
              ],
              "aggregate": {
                "as": [
                  "sum_TicketCost"
                ],
                "fields": [
                  "TicketCost"
                ],
                "ops": [
                  "sum"
                ]
              }
            }
          },
          "signals": [
            {
              "name": "barWidth",
              "update": "scale('x', parent['sum_TicketCost'])"
            },
            {
              "name": "barHeight",
              "update": "bandWidth"
            }
          ],
          "encode": {
            "update": {
              "y": {
                "signal": "scale('y', datum['bin_maxbins_20_Age'])-bandWidth"
              },
              "height": {
                "signal": "bandWidth"
              },
              "x": {
                "signal": "scale('x', 0)"
              },
              "width": {
                "signal": "scale('x', datum['sum_TicketCost'])"
              }
            }
          },
          "marks": [
            {
              "name": "treemapMarks",
              "type": "rect",
              "from": {
                "data": "treemapData"
              },
              "encode": {
                "update": {
                  "x": {
                    "field": "x0"
                  },
                  "x2": {
                    "field": "x1"
                  },
                  "y": {
                    "field": "y0"
                  },
                  "y2": {
                    "field": "y1"
                  }
                }
              }
            }
          ],
          "data": [
            {
              "name": "treemapData",
              "source": "bandfacet_0",
              "transform": [
                {
                  "type": "nest"
                },
                {
                  "type": "treemap",
                  "field": "TicketCost",
                  "sort": {
                    "field": "value",
                    "order": "descending"
                  },
                  "round": true,
                  "method": "squarify",
                  "padding": 1,
                  "size": [
                    {
                      "signal": "barWidth"
                    },
                    {
                      "signal": "barHeight"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ],
      "axes": [
        {
          "scale": "x",
          "orient": "bottom",
          "gridScale": "y",
          "grid": true,
          "tickCount": {
            "signal": "ceil(child_width/40)"
          },
          "domain": false,
          "labels": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {
        "data": "source_0",
        "field": "sum_TicketCost"
      },
      "range": [
        0,
        {
          "signal": "child_width"
        }
      ],
      "nice": true,
      "zero": true
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "signal": "[child_bin_maxbins_20_Age_bins.start, child_bin_maxbins_20_Age_bins.stop]"
      },
      "range": [
        {
          "signal": "child_height"
        },
        0
      ],
      "bins": {
        "signal": "child_bin_maxbins_20_Age_bins"
      },
      "zero": false
    },
    {
      "name": "quantBand",
      "type": "band",
      "domain": {
        "data": "seq",
        "field": "data",
        "sort": true
      },
      "range": [
        {
          "signal": "child_height"
        },
        0
      ],
      "padding": {
        "signal": "bandPadding"
      }
    }
  ]
}