{
  "name": "@msrvida/mdvega-sanddance",
  "version": "1.0.0",
  "description": "SandDance plugin for mdvega",
  "main": "index.js",
  "dependencies": {
    "@msrvida/sanddance": "^4"
  },
  "devDependencies": {
    "@msrvida/chart-types": "^1",
    "@msrvida/mdvega": "^1",
    "vega-typings": "~1.3.1"
  },
  "scripts": {
    "watch-typescript": "tsc -p . -w",
    "build-typescript": "tsc -p .",
    "bundle": "parcel build ./dist/es6/index.js --no-autoinstall --no-cache",
    "build:sanddance-integration": "npm run build-typescript && npm run bundle"
  },
  "targets": {
    "main": false,
    "types": false,
    "umd": {
      "distDir": "./dist/umd",
      "optimize": false,
      "sourceMap": false,
      "includeNodeModules": {
        "react": false,
        "react-dom": false
      }
    }
  },
  "umd": "./dist/umd/mdvega-sanddance.js",
  "keywords": [
    "markdown",
    "vega"
  ],
  "author": "Dan Marshall",
  "license": "MIT"
}